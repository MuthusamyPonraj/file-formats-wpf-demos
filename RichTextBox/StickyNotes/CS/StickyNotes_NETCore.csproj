<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>StickyNotes</AssemblyName>
    <RootNamespace>StickyNotes</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;NETCORE;</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NETCORE;</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Compression.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.OfficeChart.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.DocIO.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfRichTextBoxAdv.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfRichTextRibbon.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="App.ico" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Assets\Blue.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Assets\Green.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Assets\Pink.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Assets\Purple.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Assets\White.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Assets\Yellow.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
  </ItemGroup>
</Project>