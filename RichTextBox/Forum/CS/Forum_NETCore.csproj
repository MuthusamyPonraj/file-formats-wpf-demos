<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>Forum</AssemblyName>
    <RootNamespace>Forum</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;NETCORE;</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NETCORE;</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="Assets\InitialQuery.docx" />
    <EmbeddedResource Include="Assets\ReplyContent.docx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.OfficeChart.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.DocIO.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfRichTextBoxAdv.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="App.ico" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Assets\PostedBy.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Assets\User32.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Assets\User48.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
  </ItemGroup>
</Project>